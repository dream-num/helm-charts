# version
UNIVERSER_VERSION=latest
COLLABORATION_SERVER_VERSION=latest
COLLABORATION_DEMO_VERSION=latest
UNIVER_WORKER_VERSION=latest
UNIVER_WORKER_EXCHANGE_VERSION=latest
UNIVERSER_SQL_VERSION=0.0.5
POSTGRES_VERSION=16.1
MYSQL_VERSION=8.3.0
RABBITMQ_VERSION=3-management
