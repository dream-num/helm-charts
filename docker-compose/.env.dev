CLICKHOUSE_PASSWORD=${CLICKHOUSE_PASSWORD}

# admin config
UNIVERSER_ADMIN_USERNAME=admin
UNIVERSER_ADMIN_PASSWORD=${UNIVERSER_ADMIN_PASSWORD}

# grafana config
GRAFANA_USERNAME=admin
GRAFANA_PASSWORD=${GRAFANA_PASSWORD}

# s3 config
S3_USER=${S3_USER}
