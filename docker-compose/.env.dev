S3_REGION=${S3_REGION}
S3_PATH_STYLE=${S3_PATH_STYLE}
S3_ENDPOINT=${S3_ENDPOINT}
S3_ENDPOINT_PUBLIC=${S3_ENDPOINT_PUBLIC}
S3_DEFAULT_BUCKET=${S3_DEFAULT_BUCKET}

# temporal config
TEMPORAL_HOST=univer-temporal
TEMPORAL_PORT=7233
